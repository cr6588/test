<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <properties>
        <property name="dialect" value="mysql" />
    </properties>
    <settings>
        <setting name="logImpl" value="LOG4J"/>
    </settings>
    <typeAliases>
<!--         <package name="com.cheny.web.bean"/> -->
        <typeAlias alias="int" type="java.lang.Integer" />
        <typeAlias alias="long" type="java.lang.Long" />
        <typeAlias alias="float" type="java.lang.Float" />
        <typeAlias alias="double" type="java.lang.Double" />
        <typeAlias alias="boolean" type="java.lang.Boolean" />
        <typeAlias alias="string" type="java.lang.String" />
        <typeAlias alias="date" type="java.util.Date" />
        <typeAlias alias="map" type="java.util.Map" />
        <typeAlias alias="list" type="java.util.List" />
        <typeAlias alias="linkMap" type="java.util.LinkedHashMap" />

        <typeAlias alias="adminRole" type="com.cheny.admin.bean.AdminRole" />
        <typeAlias alias="adminUser" type="com.cheny.admin.bean.AdminUser" />
        <typeAlias alias="adminUserRole" type="com.cheny.admin.bean.AdminUserRole" />
    </typeAliases>
</configuration>